{
    "name": "enflow/nova-uploadcare-field",
    "description": "A Laravel Nova Uploadcare file upload field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Enflow\\NovaUploadcareField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enflow\\NovaUploadcareField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
